package{	import fl.transitions.Tween;	import fl.transitions.easing.*;	import flash.display.MovieClip;	import Math;		import resolumeCom.*;	import resolumeCom.parameters.*;	import resolumeCom.events.*;	public class Tweener extends MovieClip	{		private var ht1:Tween;		private var wt1:Tween;		private var ht2:Tween;		private var wt2:Tween;		private var ht3:Tween;		private var wt3:Tween;		private var ht4:Tween;		private var wt4:Tween;		private var ht5:Tween;		private var wt5:Tween;		private var resolume:Resolume = new Resolume();		private var tempo:FloatParameter = resolume.addFloatParameter("Tempo", 0.6);				public function Tweener()		{			resolume.addParameterListener(parameterChanged);						ht1 = new Tween (s1, "x", Strong.easeInOut, -20, 660, 4, true);			ht1.looping = true;			wt1 = new Tween (s1, "width", Strong.easeOut, 20, 10 + (400 * Math.random()), 4, true);			wt1.looping = true;						ht2 = new Tween (s2, "x", Strong.easeInOut, -20, 660, 3, true);			ht2.looping = true;			wt2 = new Tween (s2, "width", Strong.easeOut, 20, 10 + (400 * Math.random()), 3, true);			wt2.looping = true;						ht3 = new Tween (s3, "x", Strong.easeInOut, -20, 660, 5, true);			ht3.looping = true;			wt3 = new Tween (s3, "width", Strong.easeOut, 20, 10 + (400 * Math.random()), 5, true);			wt3.looping = true;						ht4 = new Tween (s4, "x", Strong.easeInOut, -20, 660, 7, true);			ht4.looping = true;			wt4 = new Tween (s4, "width", Strong.easeOut, 20, 10 + (400 * Math.random()), 7, true);			wt4.looping = true;						ht5 = new Tween (s5, "x", Strong.easeInOut, -20, 660, 6, true);			ht5.looping = true;			wt5 = new Tween (s5, "width", Strong.easeOut, 20, 10 + (400 * Math.random()), 6, true);			wt5.looping = true;					}				public function parameterChanged(e:ChangeEvent):void		{			if (e.object == tempo)			{				ht1.duration = 0.1 + (11 * (1 - tempo.getValue()));				ht2.duration = 0.1 + (12 * (1 - tempo.getValue()));				ht3.duration = 0.1 + (13 * (1 - tempo.getValue()));				ht4.duration = 0.1 + (14 * (1 - tempo.getValue()));				ht5.duration = 0.1 + (15 * (1 - tempo.getValue()));								wt1.duration = 0.1 + (11 * (1 - tempo.getValue()));				wt2.duration = 0.1 + (12 * (1 - tempo.getValue()));				wt3.duration = 0.1 + (13 * (1 - tempo.getValue()));				wt4.duration = 0.1 + (14 * (1 - tempo.getValue()));				wt5.duration = 0.1 + (15 * (1 - tempo.getValue()));			}		}	}}