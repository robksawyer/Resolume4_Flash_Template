package{	import flash.display.MovieClip;	import flash.xml.*;		public class RssTitle extends MovieClip	{		private var rssData:XMLList;		private var currentTitle:int;				public function RssTitle()		{					}				public function UpdateRssData(_rssData:XMLList)		{			currentTitle = 0;			rssData = _rssData;		}				public function ShowRssTitle()		{			if (currentTitle >= rssData.length()) currentTitle = 0;			rss_text.text = rssData[currentTitle].title;			currentTitle++;					}	}	}